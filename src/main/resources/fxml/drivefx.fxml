<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.ProgressIndicator?>
<?import javafx.scene.layout.*?>
<VBox xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/null"
      fx:controller="tutor.programacion.DriveFXController">
    <children>
        <HBox alignment="CENTER_LEFT">
            <children>
                <Label text="DriveFX"/>
                <ProgressIndicator fx:id="progres" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
                                   minWidth="-Infinity" prefHeight="20.0" prefWidth="20.0">
                    <HBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                    </HBox.margin>
                </ProgressIndicator>
                <Hyperlink alignment="CENTER_RIGHT" maxWidth="1.7976931348623157E308" onAction="#onRegresar"
                           text="Ir al Inicio" HBox.hgrow="ALWAYS"/>
            </children>
        </HBox>
        <ListView fx:id="listView" VBox.vgrow="ALWAYS"/>
    </children>
    <padding>
        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
    </padding>
</VBox>
